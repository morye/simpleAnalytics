{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": "last 2 versions"
      },
      "modules": false,
      "loose": false
    }]
  ],
  "plugins": [
    "transform-class-properties"
  ],
  "env": {
    "development": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}

